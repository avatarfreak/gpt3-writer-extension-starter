{
  "name": "GPT-3 Writer",
  "description": "Imagination has no boundaries, Write your thoughts anywhere!",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "83": "assets/83.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Generate blog post"
  },
  "background": {
    "service_worker": "./scripts/contextMenuServiceWorker.js"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ]
}